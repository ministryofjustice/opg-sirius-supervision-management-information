{{ define "main" }}
    <div id="main-container" class="app-width-container govuk-width-container">
        <main class="govuk-main-wrapper app-main-class" role="main">
            {{ if .SuccessMessage }}
                {{ template "success-banner" . }}
            {{ end }}
            <div id="main-content">
                {{ block "main-content" . }}{{ end }}
            </div>
        </main>
    </div>
{{ end }}